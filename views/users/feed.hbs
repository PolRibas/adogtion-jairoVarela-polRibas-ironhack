{{>navUser}}
<h1 class="uk-heading">Dogs</h1>
<section class="dog-list">

{{#each notLikedDogs}}
<article class="dog-article">
    <h2 class="uk-heading-divider">{{name}}</h2>
    <img src="{{image}}" alt="{{name}}">

    {{#if breed}}
    <p>Breed: {{breed}}</p>
    {{/if}}

     <p>Age: {{age}}</p>
    <form class='chat-form' action='/api/likeDog'>

        <input name="shelter" value="{{shelter.[0]._id}}" style="display:none;">
        <input name="dog" value="{{_id}}" style="display:none;">
        <button type="submit">Favorites</button>
    </form>
</article>
{{/each}}

<script src="/javascripts/users/like.js"></script>

