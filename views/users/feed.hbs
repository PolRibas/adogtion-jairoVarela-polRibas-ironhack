{{>navUser}}
<h1 class="uk-heading">My Dogs</h1>
<section class="dog-list">
{{#each dogs}}
<article>
    <h2 class="uk-heading-divider">{{name}}</h2>
    <img class="dog-img" src="{{image}}" alt="{{name}}">
    {{#if breed}}
    <p>Breed: {{breed}}</p>
    {{/if}}
     <p>Age: {{age}}</p>
    <form action='/api/likeDog' method="POST">
        <input name="shelter" value="{{shelter.[0]._id}}" style="display:none;">
        <input name="dog" value="{{_id}}" style="display:none;">
        <button type="submit">Favorites</button>
    </form>
</article>
{{/each}}
</section>